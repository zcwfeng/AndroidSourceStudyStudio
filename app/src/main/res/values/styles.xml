<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
    </style>
    <declare-styleable name="CircleView">
        <attr name="cv_color" format="color" />
        <attr name="cv_radius" format="integer" />
    </declare-styleable>


    <!-- Base application theme. -->
    <style name="AppTheme2" parent="Theme.AppCompat">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/color_primary_green</item>
        <item name="colorPrimaryDark">@color/color_primary_green_dark</item>
        <item name="android:textColorPrimary">@android:color/white</item>
        <item name="android:windowNoTitle">true</item>
        <item name="windowActionBar">false</item>

        <item name="android:listDivider">@drawable/divider_bg</item>
    </style>
</resources>
